{
    "version": 2,
    "builds": [
        {
            "src": "./index.js",
            "use": "@vercel/node",
            "config": {
                "cmd": "npm run migrate"
            }
        }
    ],
    "routes": [
        {
          "src": "/(.*)",
          "dest": "index.js"
        }
      ]
}